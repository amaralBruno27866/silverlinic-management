  "terminal.integrated.defaultProfile.linux": "pwsh",
  "terminal.integrated.profiles.linux": {
    "pwsh": {
      "path": "pwsh",
      "args": ["-NoLogo"],
      "icon": "terminal-powershell"
    },
    "bash": {
      "path": "bash", 
      "icon": "terminal-bash"
    }
  },
  "terminal.integrated.cwd": "${workspaceFolder}",
  "terminal.integrated.env.linux": {
    "PWSH_DEVELOPMENT": "1"
  },
  "powershell.powerShellDefaultVersion": "PowerShell",
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/include",
    "/usr/include/c++/13",
    "/usr/include/x86_64-linux-gnu/c++/13",
    "/usr/include"
  ],
  "C_Cpp.default.compilerPath": "/usr/bin/g++",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
  "cmake.buildDirectory": "${workspaceFolder}/build_test",
  "cmake.configureOnOpen": true,
  "cmake.configureArgs": [
    "-DCMAKE_BUILD_TYPE=Debug"
  ],
  "cmake.buildArgs": [
    "--config",
    "Debug"
  ],
  "cmake.parallelJobs": 4,
  "cmake.generator": "Unix Makefiles",
  "cmake.sourceDirectory": "${workspaceFolder}",
  "cmake.preferredGenerators": [
    "Unix Makefiles",
    "Ninja"
  ],
  "cmake.copyCompileCommands": "${workspaceFolder}/compile_commands.json",
  "cmake.buildOnOpen": false,
  "cmake.clearOutputBeforeBuild": true,
  "files.exclude": {
    "**/build/": true,
    "**/build_test/CMakeFiles": true,
    "**/compile_commands.json": true,
    "**/*.vcxproj": true,
    "**/*.vcxproj.filters": true,
    "**/*.sln": true
  },
  "search.exclude": {
    "**/build/": true,
    "**/build_test/": true
  },
  "cmakeExplorer.buildDir": "${workspaceFolder}/build_test",
  "cmakeExplorer.cmakeCommand": "cmake",
  "cmakeExplorer.ctestCommand": "ctest",
  "cmakeExplorer.suiteDelimiter": ".",
  "cmakeExplorer.testNameRegex": ".*",
  "cmakeExplorer.parallelJobs": 4
}